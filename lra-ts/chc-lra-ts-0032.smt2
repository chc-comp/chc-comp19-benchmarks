(set-logic HORN)
(declare-fun itp (Real Real Real Real) Bool)
(declare-fun fail () Bool)
(assert (forall ((i1 Real) (i2 Real) (N1 Real) (N2 Real))
  (=> (and (= N1 N2) (= i1 N1) (= i2 0)) (itp i1 i2 N1 N2))))
(assert (forall ((i1 Real) (i2 Real) (i1p Real) (i2p Real) (N1 Real) (N2 Real))
  (=> (and (itp i1 i2 N1 N2)
           (> i1 0)
           (< i2 N1)
           (= i1p (- i1 1))
           (= i2p (+ i2 1)))
      (itp i1p i2p N1 N2))))
(assert (forall ((N2 Real) (i2 Real) (N1 Real) (i1 Real))
  (let ((a!1 (and (itp i1 i2 N1 N2) (not (= (= i2 N1) (= i1 0))))))
    (=> a!1 false))))

(check-sat)
(exit)
