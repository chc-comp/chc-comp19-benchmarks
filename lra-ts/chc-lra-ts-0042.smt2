(set-logic HORN)
(declare-fun inv (Bool Bool Bool Bool Real Real Real Real) Bool)
(declare-fun fail () Bool)
(assert (forall ((x Bool) (y Bool) (z Bool) (v Bool)) (inv x y z v 0 0 0 0)))
(assert (forall ((x Bool)
         (y Bool)
         (z Bool)
         (v Bool)
         (a Real)
         (b Real)
         (c Real)
         (d Real)
         (x1 Bool)
         (y1 Bool)
         (z1 Bool)
         (v1 Bool)
         (a1 Real)
         (b1 Real)
         (c1 Real)
         (d1 Real))
  (let ((a!1 (or (and x1 (= a1 (+ a 1)) (= b1 b) (= c1 c) (= d1 d))
                 (and y1 (= a1 a) (= b1 (+ b 1)) (= c1 c) (= d1 d))
                 (and z1 (= a1 a) (= b1 b) (= c1 (+ c 1)) (= d1 d))
                 (and v1 (= a1 a) (= b1 b) (= c1 c) (= d1 (+ d 1))))))
    (=> (and (inv x y z v a b c d) a!1) (inv x1 y1 z1 v1 a1 b1 c1 d1)))))
(assert (forall ((a Real) (b Real) (c Real) (d Real) (x Bool) (y Bool) (z Bool) (v Bool))
  (=> (and (inv x y z v a b c d) (> (+ a b c d) 0) (not (or x y z v))) false)))

(check-sat)
(exit)
